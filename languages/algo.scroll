code
 1 TITLE TRABB PARDO-KNUTH ALGORITHM
  2 SUBSCript I,J
  3 DATA A(11)
  4 FORMAt FI(2DT), FLARGE(3D)
  5 PROCEDURE F(T=Z)
  6 BEGIN
  7 Z=SQRT(ABS(T))+5*T^3
  8 END
  9 FOR I=0(1)10
 10 A[I]=KEYBD
 11 FOR J=0(1)10 BEGIN
 12 I=J-10
 13 F(A[I]=Y)
 14 PRINT(FI)=I
 15 IF Y &gt; 400
 16 GO TO LARGE
 17 PRINT(FL)=Y
 18 GO TO NEXT
 19 LARGE: PRINT(FLARGE)=999
 20 NEXT: CARR(1) END
 21 2END





