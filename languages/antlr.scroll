exampleCodeHeader Example from the web:
code
 /** Taken from &quot;The Definitive ANTLR 4 Reference&quot; by Terence Parr */
 // Derived from http://json.org
 grammar JSON;
 json
    : value
    ;
 obj
    : &#39;{&#39; pair (&#39;,&#39; pair)* &#39;}&#39;
    | &#39;{&#39; &#39;}&#39;
    ;
 pair
    : STRING &#39;:&#39; value
    ;
 arr
    : &#39;[&#39; value (&#39;,&#39; value)* &#39;]&#39;
    | &#39;[&#39; &#39;]&#39;
    ;
 value
    : STRING
    | NUMBER
    | obj
    | arr
    | &#39;true&#39;
    | &#39;false&#39;
    | &#39;null&#39;
    ;
 
 STRING
    : &#39;&quot;&#39; (ESC | SAFECODEPOINT)* &#39;&quot;&#39;
    ;
 
 fragment ESC
    : &#39;\\&#39; ([&quot;\\/bfnrt] | UNICODE)
    ;
 fragment UNICODE
    : &#39;u&#39; HEX HEX HEX HEX
    ;
 fragment HEX
    : [0-9a-fA-F]
    ;
 fragment SAFECODEPOINT
    : ~ [&quot;\\\u0000-\u001F]
    ;
 
 NUMBER
    : &#39;-&#39;? INT (&#39;.&#39; [0-9] +)? EXP?
    ;
 
 fragment INT
    : &#39;0&#39; | [1-9] [0-9]*
    ;
 
 // no leading zeros
 
 fragment EXP
    : [Ee] [+\-]? INT
    ;
 
 // \- since - means &quot;range&quot; inside [...]
 
 WS
    : [ \t\n\r] + -&gt; skip
    ; 

