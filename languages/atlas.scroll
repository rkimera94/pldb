 rule TreeNodeRoot2RootElement {
  from
    rt : MMTree!Node (rt.isTreeNodeRoot()) 
  to
    lstRt : MMElementList!RootElement (
      name &lt;- rt.name,
      elements &lt;- elmLst
    ),
    elmLst : distinct MMElementList!CommonElement foreach(leaf in rt.getAllChildren())(
      name &lt;- leaf.name
    )
 }

