 object SquareRoot extends Baysick {
   def main(args:Array[String]) = {
     10 PRINT &quot;Enter a number&quot;
     20 INPUT &#39;n
     30 PRINT &quot;Square root of &quot; % &quot;&#39;n is &quot; % SQRT(&#39;n)
     40 END
     RUN
   }
 }





