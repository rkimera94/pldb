code
 &quot;Test function for Ceylon&quot;
 by (&quot;Enrique&quot;)
 shared void test() {
     print(&quot;test&quot;);
 }
 
 &quot;Test class for Ceylon&quot;
 shared class Test(name) satisfies Comparable&lt;Test&gt; {
     shared String name;
     shared actual String string = &quot;Test ``name``.&quot;;
 
     shared actual Comparison compare(Test other) {
         return name&lt;=&gt;other.name;
     }
 }
 

