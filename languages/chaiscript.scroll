exampleCodeHeader Example from the web:
code
 #include &lt;chaiscript/chaiscript.hpp&gt;
 std::string helloWorld(const std::string &amp;t_name) {
   return &quot;Hello &quot; + t_name + &quot;!&quot;;
 }
 int main() {
   chaiscript::ChaiScript chai;
   chai.add(chaiscript::fun(&amp;helloWorld), &quot;helloWorld&quot;);
   chai.eval(R&quot;(
     puts(helloWorld(&quot;Bob&quot;));
   )&quot;);
 }





