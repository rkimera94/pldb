 C     Multiply vector by matrix
       REAL M(,), V(), R()
       R = SUM(M*MATR(A))
 
 C     Converge to a Laplace potential in an area
       REAL P(,), OLD_P(,)
       LOGICAL INSIDE(,)
       DO 1 K = 1, ITERATIONS
       OLD_P = P
       P(INSIDE) = 0.25*(P(,+)+P(,-)+P(+,)+P(-,))
       IF (MAX(ABS(P-OLD_P)) .LT. EPS) RETURN
     1 CONTINUE

