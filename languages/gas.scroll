code
 .global	_start
 
 .text
 _start:
 	movl  $4, %eax
 	movl  $1, %ebx
 	movl  $msg, %ecx
 	movl  $len, %edx
 	int   $0x80
 
 	movl  $1, %eax
 	movl  $0, %ebx
 	int   $0x80
 .data
 msg:
 	.ascii  &quot;Hello, world!\n&quot;
 	len =   . - msg



















