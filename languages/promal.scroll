code
 PROGRAM SIEVE
    ; Sieve of Eratosthenes Benchmark
    ; test (BYTE magazine)
    ; 10 iterations, 1800 element array.
  INCLUDE LIBRARY
  CON SIZE=1800
  WORD I
  WORD J
  WORD PRIME
  WORD K
  WORD COUNT
  BYTE FLAGS[SIZE]
  
  BEGIN
  OUTPUT &quot;10 ITERATIONS&quot;
  FOR J= 1 TO 10
    COUNT=0
    FILL FLAGS, SIZE, TRUE
    FOR I= 0 TO SIZE
      IF FLAGS[I]
        PRIME=I+I+3
        K=I+PRIME
        WHILE K &lt;= SIZE
          FLAGS[K]=FALSE
          K=K+PRIME
        COUNT=COUNT+1
  OUTPUT &quot;#C#I PRIMES&quot;, COUNT
  END

