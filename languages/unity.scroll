code
 Program shortestpath2
 declare
     n: integer,
     D: array [0..n-1, 0..n-1] of integer
 initially
     n = 10 #
     &lt;|| i,j : 0 &lt;= i &lt; n and 0 &lt;= j &lt; n ::
         D[i,j] = rand() % 10 &gt;
 assign
     &lt;|| i,j : 0 &lt;= i &lt; n and 0 &lt;= j &lt; n ::
         D[i,j] := min(D[i,j], &lt;min k : 0 &lt;= k &lt; n :: D[i,k] + D[k,j] &gt;) &gt;
 end

