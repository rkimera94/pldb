 using &lt;stdio.h&gt;::{printf}
 
 export fn main() -&gt; int {
     let r = Random{
         num: 42,
     };
     printf(&quot;your lucky number: %u\n&quot;, r.gen());
     return 0;
 }
 
 struct Random {
     u32 num;
 }
 
 fn gen(Random *self) -&gt; u32 {
     return self-&gt;num;
 }





